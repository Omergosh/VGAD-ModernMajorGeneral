        else
            vspeed -= general_accel;
    } else if (vspeed &lt; 0){
        if (vspeed + general_accel &gt;= 0)
            vspeed = 0;
        else
            vspeed += general_accel;
    }
}

// Make sure we can't go off screen horizontally
var next_y = y + vspeed;
var half_sprite_height = abs(sprite_height) / 2;
