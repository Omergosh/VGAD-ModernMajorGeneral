            <string>/// Move general
//If keys are pressed, change momentum, else maintain momentum
// Horizontal
if keyboard_check(ord('A')) {
    if (hspeed - general_accel &lt;= -general_speed)
        hspeed = -general_speed;
    else
        hspeed -= general_accel;
} else if keyboard_check(ord('D')) {
    if (hspeed + general_accel &gt;= general_speed)
        hspeed = general_speed;
    else
        hspeed += general_accel;
} else {
    // Do we want a separate de-accel var?
    if (hspeed &gt; 0) {
        if (hspeed - general_accel &lt;= 0)
            hspeed = 0;
        else
            hspeed -= general_accel;
    } else if (hspeed &lt; 0){
        if (hspeed + general_accel &gt;= 0)
            hspeed = 0;
        else
            hspeed += general_accel;
    }
}

// Make sure we can't go off screen horizontally
var next_x = x + hspeed;
var half_sprite_width = abs(sprite_width) / 2;
var min_x = 0; // Change me?
var max_x = room_width - sprite_get_width(object_get_sprite(obj_hive));

if (next_x - half_sprite_width &lt;= min_x) {
    x = min_x + half_sprite_width;
    hspeed = 0;
} else if (next_x + half_sprite_width &gt;= max_x) {
    x = max_x - half_sprite_width;
    hspeed = 0;
}

// Vertical
if keyboard_check(ord('W')) {
    if (vspeed - general_accel &lt;= -general_speed)
        vspeed = -general_speed;
    else
        vspeed -= general_accel;
} else if keyboard_check(ord('S')) {
